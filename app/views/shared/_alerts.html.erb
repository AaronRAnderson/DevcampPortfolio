<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Aaron Anderson Portfolio"), sticky: false %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title: "Aaron Anderson Portfolio"), sticky: false %>
<% else %>
  <%= js add_gritter(flash[:notice], title: "Aaron Anderson Portfolio"), sticky: false %>
<% end %>